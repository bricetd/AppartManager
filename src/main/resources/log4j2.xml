<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
		<File name="LogFile" fileName="batterymanager.log">
			<PatternLayout
				pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<!-- <Logger name="com.mby" level="debug" additivity="false"> -->
		<!-- <AppenderRef ref="LogFile" /> -->
		<!-- </Logger> -->
		<Root level="debug">
			<AppenderRef ref="LogFile" />
		</Root>
	</Loggers>
</Configuration>